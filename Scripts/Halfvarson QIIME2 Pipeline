qiime tools import \
  --type "SampleData[SequencesWithQuality]" \
  --input-format SingleEndFastqManifestPhred33V2 \
  --input-path halfvarson_manifest_filtered.tsv \
  --output-path ./halfvarson_demux_seqs.qza

